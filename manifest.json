{
    "name": "Dark Theme",
    "description": "Make Websites Dark",
    "version": "1.0.0",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Dark Theme",
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "permissions":  [
        "https://*/*",
        "http://*/*",
        "tabs"
    ]
}